<% provide :button_text, "Edit" %>

<h1>Subscriptions#Edit</h1>

<%= form_with(model: [@user, @subscription],url: user_edit_subscription_url ,local: true) do |f| %>
  <%= render 'utils/error_messages', object: f.object %>
  <%= f.label :duration %>
  <%= f.text_field :duration, required: true %>
  <%= f.submit yield(:button_text) %>
<% end %>
